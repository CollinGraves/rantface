<!DOCTYPE html>
<html>
<head>
  <title>App</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<%= render "partials/navbar" %>
<% if flash.any? %>
  <% flash.each do |msg| -%>
    <%= content_tag :div, msg %>
  <% end -%>
<% end %>

<%= yield %>

</body>
</html>
